{
  "name": "@nekogaming/eslint-config-react-native",
  "version": "4.1.1",
  "description": "Pluggable ESLint config for React Native that you can import, extend and override",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1", 
    "prepare": "js-yaml index.yaml > index.json && js-yaml style-guide/index.yaml > style-guide/index.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thakyz/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig",
    "react",
    "reactjs",
    "react-native"
  ],
  "author": "thakyz@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thakyz/eslint-config/issues"
  },
  "homepage": "https://github.com/thakyz/eslint-config/tree/master/packages/react-native#readme",
  "dependencies": {
    "eslint": "^8.41.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  },
  "peerDependencies": {
    "eslint": ">=6"
  }
}
